  "paths": {
    "/api/auth/register": {
      "post": {
        "tags": [
          "认证接口"
        ],
        "summary": "用户注册",
        "description": "注册新用户账号",
        "operationId": "registerUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponseString"
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/login": {
      "post": {
        "tags": [
          "认证接口"
        ],
        "summary": "用户登录",
        "description": "使用邮箱和密码进行登录",
        "operationId": "authenticateUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponseJwtAuthenticationResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/me": {
      "get": {
        "tags": [
          "认证接口"
        ],
        "summary": "获取当前用户信息",
        "description": "获取当前登录用户的信息并重置JWT",
        "operationId": "getCurrentUser",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponseJwtAuthenticationResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "RegisterRequest": {
        "required": [
          "email",
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "maxLength": 20,
            "minLength": 3,
            "type": "string",
            "description": "用户名",
            "example": "johndoe",
            "refType": null
          },
          "password": {
            "maxLength": 20,
            "minLength": 6,
            "type": "string",
            "description": "密码",
            "example": "password123",
            "refType": null
          },
          "email": {
            "type": "string",
            "description": "邮箱地址",
            "example": "john@example.com",
            "refType": null
          }
        },
        "description": "用户注册请求"
      },
      "ApiResponseString": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "响应码",
            "format": "int32",
            "example": 200
          },
          "message": {
            "type": "string",
            "description": "响应消息",
            "example": "操作成功"
          },
          "data": {
            "type": "string",
            "description": "响应数据"
          },
          "timestamp": {
            "type": "integer",
            "description": "时间戳",
            "format": "int64",
            "example": 1625644800000
          }
        },
        "description": "统一响应格式"
      },
      "LoginRequest": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "邮箱地址",
            "example": "user@example.com",
            "refType": null
          },
          "password": {
            "maxLength": 20,
            "minLength": 6,
            "type": "string",
            "description": "密码",
            "example": "password123",
            "refType": null
          }
        },
        "description": "用户登录请求"
      },
      "ApiResponseJwtAuthenticationResponse": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "响应码",
            "format": "int32",
            "example": 200
          },
          "message": {
            "type": "string",
            "description": "响应消息",
            "example": "操作成功"
          },
          "data": {
            "$ref": "#/components/schemas/JwtAuthenticationResponse"
          },
          "timestamp": {
            "type": "integer",
            "description": "时间戳",
            "format": "int64",
            "example": 1625644800000
          }
        },
        "description": "统一响应格式"
      },
      "JwtAuthenticationResponse": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "访问令牌",
            "refType": null
          },
          "tokenType": {
            "type": "string",
            "description": "令牌类型",
            "example": "Bearer",
            "refType": null
          },
          "expiresIn": {
            "type": "integer",
            "description": "令牌过期时间(秒)",
            "format": "int64",
            "example": 86400,
            "refType": null
          },
          "email": {
            "type": "string",
            "description": "用户邮箱",
            "refType": null
          },
          "user": {
            "$ref": "#/components/schemas/UserInfo",
            "refType": "UserInfo"
          }
        },
        "description": "JWT认证响应"
      },
      "UserInfo": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "用户ID",
            "format": "int64",
            "refType": null
          },
          "username": {
            "type": "string",
            "description": "用户名",
            "refType": null
          },
          "email": {
            "type": "string",
            "description": "邮箱",
            "refType": null
          },
          "creditBalance": {
            "type": "number",
            "description": "积分余额",
            "refType": null
          },
          "createdAt": {
            "type": "string",
            "description": "创建时间",
            "format": "date-time",
            "refType": null
          },
          "updatedAt": {
            "type": "string",
            "description": "更新时间",
            "format": "date-time",
            "refType": null
          }
        },
        "description": "用户信息响应"
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
